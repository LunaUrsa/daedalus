{
  "name": "@extension/popup",
  "version": "0.2.2",
  "description": "chrome extension popup",
  "private": true,
  "sideEffects": true,
  "files": [
    "dist/**"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "pnpm run clean&& pnpm type-check && vite build",
    "build:watch": "cross-env __DEV__=true vite build --mode development",
    "dev": "pnpm build:watch",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "pnpm lint --fix",
    "prettier": "prettier . --write --ignore-path ../../.prettierignore",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@extension/shared": "workspace:*",
    "@extension/storage": "workspace:*",
    "@extension/content-runtime-script": "workspace:*",
    "@codemirror/autocomplete": "^6.16.3",
    "@codemirror/lang-css": "^6.2.1",
    "@codemirror/lint": "^6.8.1",
    "@codemirror/view": "^6.28.2",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^5.15.20",
    "@mui/lab": "5.0.0-alpha.170",
    "@mui/material": "^5.15.20",
    "@types/uuid": "^10.0.0",
    "@uiw/codemirror-extensions-basic-setup": "^4.22.2",
    "@uiw/codemirror-themes-all": "^4.22.2",
    "@uiw/react-codemirror": "^4.22.2",
    "codemirror": "^6.0.1",
    "react-beautiful-dnd": "^13.1.1",
    "react-router-dom": "^6.23.1",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@extension/tailwindcss-config": "workspace:*",
    "@extension/tsconfig": "workspace:*",
    "@extension/vite-config": "workspace:*",
    "@types/node": "^20.14.10",
    "@types/react-beautiful-dnd": "^13.1.8"
  }
}